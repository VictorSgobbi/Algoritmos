Var
    i, numero: inteiro
    caractere: caracter
Inicio
    Para i de 1 ate 5 faca
        Escreva("Informe um número entre 65 e 119: ")
        Leia(numero)
        caractere := CARAC(numero)
        Escreval("O caractere correspondente ao número ", numero, " é: ", caractere)
    Fimpara
Fimalgoritmo
